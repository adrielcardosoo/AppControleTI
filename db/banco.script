CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE LOGINS(CODIGO INTEGER,NOME VARCHAR(100),LOGIN VARCHAR(50),SENHA VARCHAR(50))
CREATE MEMORY TABLE CLIENTES(CODIGO INTEGER,NOME VARCHAR(100),ENDERECO VARCHAR(100),BAIRRO VARCHAR(60),CIDADE VARCHAR(100),ESTADO VARCHAR(50))
CREATE MEMORY TABLE SERVICOS(CODIGO INTEGER,DESCRICAO VARCHAR(100),VALOR DECIMAL(13,2),TEMPO_MEDIO DECIMAL(13,2))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
